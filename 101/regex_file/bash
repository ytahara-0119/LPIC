echo ' >>> find . -type f '
find . -type f
echo ' >>> find . -type f | grep -E '[a-z][a-z][a-z].txt' '
find . -type f | grep -E '[a-z][a-z][a-z].txt'
echo ' >>> find . -type f | grep -E 'xy?.txt' '
find . -type f | grep -E 'xy?.txt'
echo ' >>> find . -type f | grep -E '*[xyz].txt' '
find . -type f | grep -E '*[xyz].txt'
